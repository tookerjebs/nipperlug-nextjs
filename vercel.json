{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://nipperlug.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "planner.nipperlug.com"
        }
      ]
    }
  ]
}